,MAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AACA,KARA;AAUA,IAAA,KAAA,CAAA,IAAA,0CAAA;AAAA,MAAA,MAAA,EAAA;AAAA,QAAA,KAAA,EAAA,KAAA,QAAA,EAAA;AAAA,QAAA,SAAA,EAAA,KAAA,gBAAA;AAAA;AAAA,KAAA,EAAA;AAAA,KACA,IADA,CACA,UAAA,QAAA,EAAA;AACA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,QAAA;AACA,MAAA,KAAA,CAAA,IAAA,GAAA,QAAA,CAAA,IAAA,CAAA,IAAA;AACA,KALA,EAMA,KANA,CAMA,UAAA,CAAA,EAAA;AACA,MAAA,KAAA,CAAA,MAAA,CAAA,IAAA,CAAA,CAAA;AACA,KARA;AASA,GA7CA;AA8CA,EAAA,IA9CA,kBA8CA;AACA,WAAA;AACA,MAAA,UAAA,EAAA,EADA;AAEA,MAAA,IAAA,EAAA,EAFA;AAGA,MAAA,MAAA,EAAA,EAHA;AAIA,MAAA,IAAA,EAAA,EAJA;AAKA,MAAA,QAAA,EAAA,IALA;AAMA,MAAA,KAAA,EAAA,IANA;AAOA,MAAA,SAAA,EAAA,EAPA;AAQA,MAAA,UAAA,EAAA,CACA,KADA,EAEA,KAFA,EAGA,KAHA;AARA,KAAA;AAcA,GA7DA;AA8DA,EAAA,QAAA,EAAA;AACA,IAAA,aADA,2BACA;AACA,aAAA,wBAAA;AACA,KAHA;AAIA,IAAA,OAJA,qBAIA;AACA,aAAA,kBAAA;AACA,KANA;AAOA,IAAA,SAPA,uBAOA;AACA,aAAA,oBAAA;AACA;AATA,GA9DA;AAyEA,EAAA,OAAA,EAAA;AACA,IAAA,qBADA,mCACA;AACA,aAAA,QAAA;AACA,KAHA;AAIA,IAAA,aAJA,2BAIA;AACA,aAAA,QAAA;AACA,KANA;AAOA,IAAA,QAPA,sBAOA,CAEA,CATA;AAUA,IAAA,gBAVA,8BAUA,CAEA;AAZA;AAzEA,CAAA","sourcesContent":["<template>\n  <v-container class=\"mx-auto\">\n    <v-row>\n      <v-col>\n        <span>\n          Restaurant Name: {{restaurant.name || \"restaurant.name\"}}\n        </span>\n      </v-col>\n      <v-col>\n        <span>\n          Chef On Duty: {{chef.name || \"chef.name\"}}\n        </span>\n      </v-col>\n      <v-col>\n        <span>\n          Your Waiter: {{waiter.name || \"waiter.name\"}}\n        </span>\n      </v-col>\n    </v-row>\n    <v-row>\n      <v-text-field\n        v-model=\"partyName\"\n        label=\"Enter Party Name\"\n        outlined\n      />\n    </v-row>\n    <v-row>\n      <v-select\n        :items=\"menu\"\n        label=\"Select Items to Order\"\n        outlined\n        multiple\n      />\n    </v-row>\n    <v-row>\n      <span>\n        SubTotal: {{getPriceSelectedItems()}}\n      </span>\n    </v-row>\n    <v-row>\n      <v-select\n        :items=\"tipAmounts\"\n        label=\"Tip Amount\"\n        outlined\n      />\n    </v-row>\n    <v-row>\n      <span>\n        Total: {{getTotalPrice()}}\n      </span>\n    </v-row>\n    <v-row>\n      <v-btn\n        width=\"100%\"\n        elevation=\"2\"\n        x-large\n      >Submit Order</v-btn>\n    </v-row>\n  </v-container>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: 'Restaurant',\n  props: {\n  },\n  mounted() {\n    axios.get(`http://our-url-goes-here/get-restaurant`)\n      .then(response => {\n        // JSON responses are automatically parsed.\n        console.log(response)\n        this.restaurant = response.data.restaurant\n      })\n      .catch(e => {\n        this.errors.push(e)\n      })\n\n    axios.get(`http://our-url-goes-here/get-staff`) // gets chef and waiter\n      .then(response => {\n        // JSON responses are automatically parsed.\n        console.log(response)\n        this.chef = response.data.staff.chef\n        this.waiter = response.data.staff.waiter\n      })\n      .catch(e => {\n        this.errors.push(e)\n      })\n\n    axios.get(`http://our-url-goes-here/get-menu`) // gets chef and waiter\n      .then(response => {\n        // JSON responses are automatically parsed.\n        console.log(response)\n        this.menu = response.data.menu\n      })\n      .catch(e => {\n        this.errors.push(e)\n      })\n\n    axios.post(`http://our-url-goes-here/submit-order`, { params: { party: this.getParty(), menuItems: this.getSelectedItems()}}) // gets chef and waiter\n      .then(response => {\n        // JSON responses are automatically parsed.\n        console.log(response)\n        this.menu = response.data.menu\n      })\n      .catch(e => {\n        this.errors.push(e)\n      })\n  },\n  data () {\n    return {\n      restaurant: {},\n      chef: {},\n      waiter: {},\n      menu: [],\n      subTotal: 0.00,\n      total: 0.00,\n      partyName: \"\",\n      tipAmounts: [\n        \"15%\",\n        \"20%\",\n        \"25%\"\n      ]\n    }\n  },\n  computed: {\n    getRestaurant () {\n      return \"PlaceHolder Restaurant\"\n    },\n    getChef () {\n      return \"PlaceHolder Chef\"\n    },\n    getWaiter () {\n      return \"PlaceHolder Waiter\"\n    },\n  },\n  methods: {\n    getPriceSelectedItems () {\n      return \"$36.86\"\n    },\n    getTotalPrice () {\n      return \"$40.00\"\n    },\n    getParty () {\n\n    },\n    getSelectedItems () {\n\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n\n</style>\n"],"sourceRoot":"src/components"}]}